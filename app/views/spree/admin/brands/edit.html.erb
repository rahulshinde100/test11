<%= render :partial => 'spree/admin/shared/configuration_menu' %>
<br class='clear' />
<div class='toolbar'>
  <ul class='actions header-action-links inline-menu'>
    <li style="float: right;">
      <%= button_link_to Spree.t("back_to_brands"), admin_brands_url, :icon => 'icon-arrow-left' %>
    </li>
  </ul>
  <br class='clear' />
</div>
<div data-hook="admin_brand_edit_form_header">
  <%= render "spree/shared/error_messages", :target => @brand %>
</div>
<fieldset>
<legend align="center">BRAND :: EDIT BRAND</legend>
<div data-hook="admin_brand_edit_form">
  <%= form_for(@brand, :url => object_url, :html => { :method => :put }) do |f| %>
    <%= render :partial => "form", :locals => { :f => f } %>
    <div data-hook="admin_brand_edit_form_buttons">
      <%= render :partial => "spree/admin/shared/edit_resource_links" %>
    </div>
  <% end %>
  </div>
</fieldset>
