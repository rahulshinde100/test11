<%= render :partial => 'spree/admin/shared/order_sub_menu' %>
<div style="float:right">
  <p><label>Success</label> | <%= link_to 'Pending',oms_pending_admin_orders_path, :class => 'button' %></p>
</div>
<% unless @oms_order_logs.blank? %>
  <table id="oms_order_status" class="display" >
    <thead>
      <tr>
        <th>Id</th>
        <th>Order No</th>
        <th>OMS Reference No</th>
        <th>OMS API Response</th>
        <th>OMS API Message</th>
     </tr>
    </thead>
    <tbody>
      <% @oms_order_logs.each do |order| %>
      <tr>
        <td><%= order.id%></td>
        <td><%= order.order.number%> </td>
        <td><%= order.oms_reference_number%></td>
        <td><%= order.oms_api_responce%></td>
        <td><%= order.oms_api_message%></td>
      </tr>
     <% end %>

    </tbody>
  </table>
  <%= paginate @oms_order_logs %>
<%else%>
  <h3><%= Spree.t(:no_orders)%></h3>
<%end%>