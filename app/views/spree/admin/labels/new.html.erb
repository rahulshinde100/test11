<%= render :partial => 'spree/admin/shared/product_sub_menu' %>
<% content_for :page_title do %>
  Add New Label
<% end %>

<% content_for :page_actions do %>
    <div class="toolbar" data-hook="toolbar">
      <ul class="actions">
        <li class="li_block">
            <a class="button icon-arrow-left" icon="icon-arrow-left" href="/admin/labels">Back To Label List</a>
        </li>
      </ul>
      <br class="clear" />
    </div>
<% end %>


<%= render :partial => 'spree/shared/error_messages', :locals => { :target => @label } %>

<%= form_for [:admin, @label] do |f| %>
	<fieldset data-hook="new_label">
	  <legend align="center"><%= t(:new_label) %></legend>
	  <%= render :partial => 'form', :locals => { :f => f } %>
	  <%= render :partial => 'spree/admin/shared/new_resource_links' %>
	</fieldset>
<% end %>